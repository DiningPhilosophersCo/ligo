(contract
  (fun
    rec
    'sum
    (params (var 'n (PROD int)) (var 'acc (PROD int)))
    :
    (PROD int)
    = (if (n < 1) acc (apply sum (tuple (n - 1) (acc + n)))))
  (fun
    rec
    'fibo
    (params
      (var 'n (PROD int)) (var 'n_1 (PROD int)) (var 'n_0 (PROD int)))
    :
    (PROD int)
    = (if (n < 2) n_1 (apply fibo (tuple (n - 1) (n_1 + n_0) n_1)))))
