(contract
  (type password (RECORD series: int number: int))
  (type user (RECORD id: password is_admin: bool name: string))
  (const
    (? alice)
    user
    (record
      (.= (. id) (record (.= (. series) 0) (.= (. number) 1)))
      (.= (. is_admin) true) (.= (. name) "Alice")))
  (const (? alice_admin) bool (. alice id series)))
