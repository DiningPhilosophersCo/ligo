    "@opam/odoc": "*",
    "@opam/ocamlgraph": "*",
    "@opam/ocamlfind": "*",
    "@opam/ocaml-recovery-parser": "*",
    "@opam/ocaml-migrate-parsetree": "*",
    "@opam/menhir": "*",
    "@opam/lwt": "*",
    "@opam/linenoise": "*",
    "@opam/irmin-pack": "*",
    "@opam/irmin": "*",
    "@opam/hacl-star-raw": "*",
    "@opam/getopt": "*",
    "@opam/dune": "*",
    "@opam/ctypes-foreign": "*",
    "@opam/coq": "*",
    "@opam/bls12-381-legacy": "*",
    "@opam/bisect_ppx": "*",
    "@opam/alcotest-lwt": "*",
    "@esy-ocaml/substs": "*",
