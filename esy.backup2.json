{
  "name": "ligo",
  "version": "0.31.0",
  "esy": {
    "release": {
      "bin": ["ligo"],
      "includePackages": [
        "root",
        "esy-libev",
        "@esy-ocaml/libffi",
        "@opam/hacl-star-raw",
        "@opam/conf-libev",
        "esy-gmp"
      ],
      "rewritePrefix": true
    },
    "build": "dune build -p #{self.name}",
    "buildEnv": {
      "OPAM_SWITCH_PREFIX": "#{ @opam/tezos-rust-libs.install }"
    },
    "exportedEnv": {
      "LD_LIBRARY_PATH": {
        "val": "#{@opam/hacl-star-raw.lib / 'hacl-star-raw' : esy-libev.lib : $LD_LIBRARY_PATH}",
        "scope": "global"
      },
      "DYLD_LIBRARY_PATH": {
        "val": "#{@opam/hacl-star-raw.lib / 'hacl-star-raw' : esy-libev.lib : $DYLD_LIBRARY_PATH}",
        "scope": "global"
      }
    }
  },
  "license": "MIT",
